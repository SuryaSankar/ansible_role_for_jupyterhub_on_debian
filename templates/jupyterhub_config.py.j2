c.JupyterHub.admin_users = {"{{ansible_ssh_user}}"}
c.Authenticator.whitelist = {"{{ansible_ssh_user}}"}
c.LocalAuthenticator.create_system_users = True

c.JupyterHub.proxy_cmd = ['/usr/local/bin/configurable-http-proxy']
c.JupyterHub.ssl_cert = '/etc/ssl/site-certs/{{jupyterhub_fqdn}}.crt'
c.JupyterHub.ssl_key = '/etc/ssl/site-certs/{{jupyterhub_fqdn}}.key'
c.Spawner.notebook_dir = '~/workspace'
c.JupyterHub.ip = '127.0.0.1'
c.Spawner.default_url = '/lab'
c.JupyterHub.log_level = 'DEBUG'
c.Spawner.debug = True
c.LocalProcessSpawner.debug = True